{{
  include(
    '@oe-bcl/bcl-carousel/carousel.html.twig',
    {
      title: title|default(''),
      title_tag: title_tag|default('h2'),
      title_link: title_link|default({}),
      id: id|default(''),
      autoplay: autoplay|default(true),
      dark: dark|default(false),
      active_item: active_item|default(1),
      items: items|default([]),
      fade: fade|default(false),
      with_controls: with_controls|default(true),
      prev_label: prev_label|default(''),
      next_label: next_label|default(''),
      with_indicators: with_indicators|default(false),
      rounded_indicators: rounded_indicators|default(false),
      disable_touch: disable_touch|default(false),
    },
    with_context: false
  )
}}
