{{
  include(
    '@oe-bcl/bcl-navigation/navigation.html.twig',
    {
      variant: variant|default('tabs'),
      align: align|default('start'),
      items: [
        link1_label is not empty ? {
          label: link1_label,
          path: link1_path|default('#home'),
          active: link1_active|default(true),
        } : null,
        link2_label is not empty ? {
          label: link2_label,
          path: link2_path|default('#features'),
          active: link2_active|default(false),
        } : null,
        link3_label is not empty ? {
          label: link3_label,
          path: link3_path|default('#pricing'),
          active: link3_active|default(false),
        } : null,
        link4_label is not empty ? {
          label: link4_label,
          path: link4_path|default('#'),
          active: link4_active|default(false),
        } : null,
      ]|filter(v => v is not null),
    },
    with_context: false
  )
}}
